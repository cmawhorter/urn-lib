{
  "name": "urn-lib",
  "version": "3.0.0",
  "description": "Creating, parsing, and validating",
  "type": "module",
  "main": "dist/main.mjs",
  "browser": "dist/web.cjs",
  "types": "dist/main.d.ts",
  "source": "src/main.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "node": {
        "require": "./dist/main.cjs",
        "default": "./dist/main.mjs"
      },
      "browser": "./dist/web.cjs",
      "default": {
        "require": "./dist/main.cjs",
        "default": "./dist/main.mjs"
      }
    },
    "./legacy": {
      "types": "./dist/legacy/index.d.ts",
      "node": {
        "require": "./dist/legacy.cjs",
        "default": "./dist/legacy.mjs"
      },
      "browser": "./dist/legacy_web.cjs",
      "default": {
        "require": "./dist/legacy.cjs",
        "default": "./dist/legacy.mjs"
      }
    },
    "./*": {
      "types": "./dist/*",
      "default": null
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "author": {
    "name": "Cory Mawhorter",
    "email": "cory.mawhorter@gmail.com"
  },
  "scripts": {
    "build": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config webpack.config.ts",
    "prepublishOnly": "npm test && npm run build",
    "upgrade": "npx npm-check -u -i '\"{chai,mocha,@types/mocha,@types/chai}\"'",
    "pretest": "npm run lint",
    "test": "npm run testonly",
    "testonly": "npx tsx ./node_modules/.bin/mocha ./test",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cmawhorter/urn-lib.git"
  },
  "bugs": {
    "url": "https://github.com/cmawhorter/urn-lib/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.2",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "@types/webpack-node-externals": "^3.0.4",
    "babel-loader": "^10.0.0",
    "chai": "4.x",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "mocha": "^10.4.0",
    "source-map-loader": "^5.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsafe": "^1.8.12",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  }
}
