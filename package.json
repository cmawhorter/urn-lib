{
  "name": "urn-lib",
  "version": "3.0.0",
  "description": "Creating, parsing, and validating",
  "type": "module",
  "main": "dist/main.cjs",
  "module": "dist/main.mjs",
  "types": "dist/main.d.ts",
  "exports": {
    "require": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.cjs"
    },
    "import": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Cory Mawhorter",
    "email": "cory.mawhorter@gmail.com",
    "url": "http://www.mawhorter.net/"
  },
  "scripts": {
    "build": "node --loader ts-node/esm node_modules/webpack-cli/bin/cli.js --config webpack.config.ts",
    "prepublishOnly": "npm test && npm run build",
    "pretest": "npm run lint",
    "test": "npm run testonly",
    "testonly": "npx tsx ./node_modules/.bin/mocha ./test",
    "lint": "eslint",
    "upgrade": "npx npm-check -u -i '\"{chai,mocha,@types/mocha,@types/chai}\"'"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cmawhorter/urn.git"
  },
  "bugs": {
    "url": "https://github.com/cmawhorter/urn/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "babel-loader": "^10.0.0",
    "@eslint/js": "^9.39.1",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.7",
    "@types/node": "^24.10.1",
    "@types/uuid": "^11.0.0",
    "chai": "4.x",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "mocha": "^10.5.2",
    "source-map-loader": "^5.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  }
}
